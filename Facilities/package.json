{
  "author": "Badr Choubai",
  "description": "TypeScript API Demo",
  "engines": {
    "node": ">=22.14.0",
    "npm": "~10"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "uuid": "^11.0.5",
    "winston": "^3.17.0"
  },
  "name": "typescript-api-demo",
  "scripts": {
    "serve": "node dist/index.js",
    "build": "tsc",
    "database:seed": "node --no-warnings --loader ts-node/esm src/database/seedDb.ts",
    "dev": "npm run develop",
    "develop": "concurrently \"npm run develop:watch\" \"npm run develop:serve\"",
    "develop:watch": "tsc --watch",
    "develop:serve": "nodemon dist/index.js",
    "lint": "eslint -c eslint.config.js --fix"
  },
  "type": "module",
  "version": "1.0.0",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.15.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0"
  }
}
